{
  "name": "json-viewer",
  "version": "0.1.10",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A JSON payload viewer for the JSON response",
  "devDependencies": {
    "bower": "^1.8.0",
    "polymer-cli": "^1.2.0",
    "shady-css-parser": "^0.1.0"
  },
  "private": false,
  "main": [
    "json-viewer.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint json-viewer.html js-max-number-error.html --rules polymer-1",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"json-viewer:local-test\"",
    "test": "node_modules/.bin/polymer test --plugin local",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/json-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/json-viewer/issues",
    "email": "arc@mulesoft.com"
  }
}